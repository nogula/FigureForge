{
  "Figure": {
    "attributes": {
      "Label": {
        "type": "string",
        "get": "get_label",
        "set": "set_label"
      },
      "Visible": {
        "type": "bool",
        "get": "get_visible",
        "set": "set_visible"
      },
      "Layout Engine": {
        "type": "choice",
        "get": "get_layout_engine",
        "set": "set_layout_engine",
        "value_options": ["constrained", "compressed", "tight", "none"]
      },

      "Title": {
        "type": "string",
        "get": "get_suptitle",
        "set": "suptitle"
      },

      "Face Color": {
        "type": "color",
        "get": "get_facecolor",
        "set": "set_facecolor"
      }
    }
  },
  "Axes": {
    "attributes": {
