        "set_parameter": "lower"
      },
      "Upper Bound": {
        "type": "float",
        "get": "get_ybound",
        "get_index": 1,
        "set": "set_ybound",
        "set_parameter": "upper"
      },
      "X Scale": {
        "type": "choice",
        "get": "get_xscale",
        "set": "set_xscale",
        "value_options": ["linear", "log", "symlog", "logit"]
      },
      "Y Scale": {
        "type": "choice",
        "get": "get_yscale",
        "set": "set_yscale",
        "value_options": ["linear", "log", "symlog", "logit"]
